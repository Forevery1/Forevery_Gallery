<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.forevery.wegallery.app">
    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.github.forevery.wegallery.app.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.github.forevery.wegallery.app.UniversalActivity"
            android:theme="@style/UniversalTheme" />
        <activity
            android:name="com.github.forevery.wegallery.gallery.Gallery_bt_MainActivity"
            android:label="包头"/>
        <activity
            android:name="com.github.forevery.wegallery.gallery.Gallery_qz_MainActivity"
            android:label="气质美女"/>
        <activity
            android:name="com.github.forevery.wegallery.gallery.Gallery_jn_MainActivity"
            android:label="集宁"/>
        <activity
            android:name="com.github.forevery.wegallery.gallery.Gallery_xmv_MainActivity"
            android:label="小美女"/>
        <activity
            android:name="com.github.forevery.wegallery.gallery.Gallery_wm_MainActivity"
            android:label="我们"/>
    </application>

</manifest>